{
  "name": "modular-synth",
  "description": "A modular synth",
  "author": "wes hatch <wes.hatch@gmail.com>",
  "version": "1.0.0",
  "private": true,
  "typings": "src/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && pnpm vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "test:e2e": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --component'",
    "test:unit": "vitest --globals --environment jsdom --root src/",
    "lint:es": "pnpx eslint . --ext .vue,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint": "oxlint"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.1",
    "firebase": "9.13.0",
    "pinia": "^2.1.7",
    "register-service-worker": "^1.7.2",
    "uuid": "^9.0.1",
    "vue": "^3.4.3"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.1",
    "@types/jsdom": "^20.0.1",
    "@vitejs/plugin-vue": "^4.6.1",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "oxlint": "^0.0.22",
    "sass": "^1.69.6",
    "start-server-and-test": "^1.15.4",
    "typescript": "^4.9.5",
    "vite": "^4.5.1",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.27"
  },
  "postcss": {
    "plugins": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
