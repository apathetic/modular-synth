{
  "name": "modular-synth",
  "description": "A modular synth",
  "author": "wes hatch <wes.hatch@gmail.com>",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "typings": "src/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "pnpm vite build",
    "buildXX": "pnpm ts && pnpm vite build",
    "preview": "vite preview",
    "ts": "vue-tsc --build",
    "test:e2e": "cypress open --component",
    "test:unit": "vitest --globals --environment jsdom --root src/",
    "lint": "oxlint . --fix -D correctness --ignore-path .gitignore"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.5",
    "pinia": "^2.3.0",
    "register-service-worker": "^1.7.2",
    "uuid": "^9.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^13.16.0",
    "jsdom": "^25.0.1",
    "oxlint": "^0.15.0",
    "sass": "^1.86.3",
    "typescript": "~5.6.3",
    "vite": "^6.0.3",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  },
  "postcss": {
    "plugins": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
