{
  "include": ["src/**/*"],
  "compilerOptions": {
    "lib": [
      // "es2015.iterable",
      "dom",
      // "es6",
      // "dom.iterable",
      // "es5",
      "es2015",
      "es2015.promise"
    ],
    "target": "es5",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,    
    "noImplicitAny" :true,
    "experimentalDecorators": true,
    "allowJs": true,
    "downlevelIteration": true
